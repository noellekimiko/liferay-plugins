<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.0+</version>
	</compatibility>
	<theme id="broken" name="Broken">
		<settings>
			<setting configurable="true" key="portlet-setup-show-borders-default" type="checkbox" value="false" />
			<setting configurable="false" key="show-site-name-default" value="false" />
			<setting configurable="false" key="show-site-name-supported" value="true" />
		</settings>
		<color-scheme id="02" name="Dark">
			<css-class>dark</css-class>
			<color-scheme-images-path>
				${images-path}/color_schemes/${css-class}
			</color-scheme-images-path>
		</color-scheme>
		<color-scheme  id="01" name="Default">
			<css-class>default</css-class>
		</color-scheme>
		<color-scheme  id="03" name="Green">
			<css-class>green</css-class>
		</color-scheme>
		<color-scheme  id="04" name="Light">
			<css-class>light</css-class>
		</color-scheme>
		<color-scheme  id="05" name="Orange">
			<css-class>orange</css-class>
		</color-scheme>
	</theme>
</look-and-feel>